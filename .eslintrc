{
  "plugins": [ "babel", "react" ],
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "semi": [ "error", "never" ],
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "no-continue": 0,
    "no-plusplus": 0,
    "max-classes-per-file": 0,
    "react/forbid-prop-types": 0,
    "react/destructuring-assignment": 0,
    "react/require-default-props": 0,
    "no-undef": 0
  }
}